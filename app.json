{
  "expo": {
    "name": "Scrubio",
    "slug": "cleaning-app",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/scrubio-logo.png",
    "scheme": "scrubio",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.madsfredag.cleaningapp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes to join a household."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.madsfredag.cleaningapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e317c3c1-9de2-490a-8e98-8076e48d06bc"
      }
    },
    "plugins": ["expo-secure-store", "expo-notifications", "expo-localization"]
  }
}
